.graf.graf-services .item .text{width:131px;left:0}.offer{padding:60px 0}.offer h1{font-size:30px;font-weight:300;color:#FFF;margin-bottom:30px;line-height:1.1}.offer p{font-size:16px;font-weight:400;color:#7a829c;margin-bottom:25px}.offer .btn{display:inline-block;color:#FFF;border:1px solid #fff;font-size:22px;border-radius:999px;padding:17px 44px;line-height:1;font-weight:200}#next-0,#prev-0,.contact-body header form{display:none}header .circle.photo{border-color:transparent}.about-body header p{color:#fff;text-align:center}.kariera-body .kariera-main-info .first-foto{position:absolute;top:-180px;left:-32px}.kariera-body header p{font-size:28px;line-height:45px;font-weight:100;color:#fff}.contact{padding:98px 0 54px}.contact h1{color:#fff;font-size:32px;font-weight:300;margin-bottom:90px}.body-reference.adonis-index-reference .filter-header{background-color:#000}.body-reference.venus-index-reference .filter-header{background-color:#59B2DB}.body-reference.flo-fit-reference .filter-header{background-color:#012B54}.body-reference.thin-form-within-reference .filter-header{background-color:#21134B}.body-reference.hot-at-home-reference .filter-header{background-color:#520E12}.body-reference.ese-reference .filter-header{background-color:#50606B}.body-reference header h1{font-size:41px;text-align:center;line-height:1.618}.body-reference header .circle span{color:#FFF;line-height:126px;font-size:40px;font-weight:200}.body-reference header p{color:#FFF;max-width:125px;margin:10px auto}.case-study{background-color:#2d3551}.case-study span{color:#fff;font-weight:200}.case-study a{color:#fff}@media (max-width:479px){.case-study{padding:34px 0}.case-study span{font-size:21px;display:block}}</style>
    {else}
        <link rel="stylesheet" href="{$basePath}/css/custom.css">
    {/if}

	<meta name="viewport" content="width=device-width">
	<meta http-equiv="X-UA-Compatible" content="IE=Edge">

    <link rel="icon" type="image/png" sizes="96x96" href="{$basePath}/favicon/favicon-96x96.png">
    <link rel="manifest" href="http://www.webvalley.cz/favicon/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{$basePath}/favicon/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <meta name="author" content="Webvalley">
    <meta name="description" content="Vývoj webu, softwaru, mobilních aplikací včetně designu, user testingu, split-testingu, analýza dat a business konzultací přesně podle vašich představ.">
    <meta name="keywords" content="webvalley, web, weby, webs, appky, apps, konverze, conversion, online">
    <meta itemprop="name" content="WEBVALLEY - Vaše nápady přetváříme na ziskový online byznys">
    <meta itemprop="description" content="Vývoj webu, softwaru, mobilních aplikací včetně designu, user testingu, split-testingu, analýza dat a business konzultací přesně podle vašich představ.">
    <!-- Open Graph data -->
    <meta property="og:title" content="WEBVALLEY - Vaše nápady přetváříme na ziskový online byznys">
    <meta property="og:site_name" content="Webvalley">
    <meta property="og:description" content="Vývoj webu, softwaru, mobilních aplikací včetně designu, user testingu, split-testingu, analýza dat a business konzultací přesně podle vašich představ.">
    <meta property="og:url" content="http://www.webvalley.cz/">
    <meta property="og:type" content="website">
    <meta property="og:image" content="{$basePath}/favicon/favicon-96x96.png">
    <!--FB-->
    <meta property="fb:admins" content="webvalley">

    <link rel="canonical" href="http://www.webvalley.cz">

    <meta name="robots" content="index, follow">

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

	{block head}{/block}
</head>

<body class="{block bodyClass}{/block}">
	<div n:foreach="$flashes as $flash" n:class="flash, $flash->type">{$flash->message}</div>

    <header class="clearfix">
        <div class="filter-header">
            <input type="checkbox" id="main-navigation" class="display-none" />
            <label for="main-navigation" class="full-page-close"></label>
            <div class="bg-menu"></div>
            <div class="container">
                {block header}
                    <div class="row">
                        <div class="span-mini-8">
                            <a class="main-logo" href="#"><img src="{$basePath}/images/wv-logo-white.svg" alt="Webvalley" width="195" height="71"></a>
                            {control breadcrumb}
                        </div>
                        <div class="span-mini-16 static-small text-right static">
                            <nav>
                                {block active}{/block}
                                <label for="main-navigation" class="label-navigation">
                                    <span class="top"></span>
                                    <span class="middle"></span>
                                    <span class="bottom"></span>
                                </label>
                                <div class="main-navigation navbar">
                                    <ul class="row">
                                        <div class="span-small-8 first">
                                            <li><a href="#">Weby</a></li>
                                            <li><a href="#">Appky</a></li>
                                            <li><a href="#">Konverze</a></li>
                                        </div>
                                        <div class="span-small-8 second">
                                            <div class="inside">
                                                <li><a href="#">Služby</a></li>
                                                <li><a href="#">Reference</a></li>
                                                <li><a href="#">O nás</a></li>
                                            </div>
                                        </div>
                                        <div class="span-small-8 third">
                                            <div class="inside">
                                                <li><a href="#">Kariéra</a></li>
                                                <li><a href="#">Partneři</a></li>
                                                <li><a href="#">Kontakt</a></li>
                                            </div>
                                        </div>
                                    </ul>
                                </div>
                            </nav>
                        </div>
                    </div>
                {/block}
            </div>

            <section>
                <div class="container">
                    {block jumbotron}{/block}
                </div>
                {block scroll}{/block}
                <a class="go-down" id="go-down" href="#main-section"><i class="tiecons tiecons-arrow-simple-normal-down"></i></a>
            </section>
        </div>
    </header>

    <section id="main-section">
        {block content}{/block}
    </section>

    <footer>
        <div class="container">
            <div class="row">
                <div class="span-medium-6">
                    <a class="main-logo" href="#"><img src="{$basePath}/images/wv-logo-white.svg" alt="Webvalley" width="195"></a>
                </div>
                <div class="span-medium-18">
                    <nav class="navbar">
                        <ul>
                            <li><a href="#">O nás</a></li>
                            <li><a href="#">Kontakt</a></li>
                            <li><a href="#">Kariéra</a></li>
                            <li><a href="#">Partneři</a></li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </footer>

    {if $showCritical}
        <script>
            window.addEventListener("load", function() {
                var cssFiles = ["custom.css"];

                for(var i = 0; i < cssFiles.length;i++) {
                    var l = document.createElement('link');
                    l.rel = 'stylesheet';
                    l.href = "{$basePath|noescape}/css/" + cssFiles[i];
                    var h = document.getElementsByTagName('head')[0];
                    h.appendChild(l);
                }
            });
        </script>
    {/if}

    <script src="https://code.jquery.com/jquery-1.12.0.min.js"></script>
    <script src="https://nette.github.io/resources/js/netteForms.min.js" async defer></script>
    <script src="{$basePath}/js/main.js"></script>
	{block scripts}{/block}
</body>
</html>
